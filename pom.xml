            </dependency>
            <dependency>
                <groupId>cn.nmmpa</groupId>
                <artifactId>common-mysql-druid</artifactId>
                <version>${common-mysql-druid.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.nmmpa</groupId>
                <artifactId>common-exception</artifactId>
                <version>${common-exception.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.nmmpa</groupId>
                <artifactId>common-redis</artifactId>
                <version>${common-redis.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.nmmpa</groupId>
