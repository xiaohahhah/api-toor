        <mybatis.springboot.version>1.3.0</mybatis.springboot.version>
        <logbackcore.version>1.2.3</logbackcore.version>
        <springcloud.version>Finchley.RELEASE</springcloud.version>
        <commons-lang3.version>3.5</commons-lang3.version>
        <jjwt.version>0.9.0</jjwt.version>
        <apollo-version>1.3.0</apollo-version>
        <servlet-api.version>4.0.1</servlet-api.version>
        <fastjson.version>1.2.56</fastjson.version>
        <pagehelper.version>1.2.3</pagehelper.version>
        <autoconfigure.version>1.2.3</autoconfigure.version>
